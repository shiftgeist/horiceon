{
  "[astro]": {
    "editor.defaultFormatter": "astro-build.astro-vscode"
  },
  "[css][html][javascript][javascriptreact][typescript][typescriptreact][markdown][scss][vue][yaml][json][jsonc][jsonl][svelte][vue][python]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "[csv][csv (semicolon)]": {
    "editor.inlayHints.maximumLength": 0
  },
  "[dockercompose][dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-containers"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[mjml]": {
    "editor.defaultFormatter": "mjmlio.vscode-mjml"
  },
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "explicit"
    }
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "mkhl.shfmt"
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "[twig]": {
    "editor.defaultFormatter": "mblode.twig-language-2"
  },
  "[yaml]": {
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.formatOnSave": false
  },
  "chat.agent.enabled": false,
  "chat.checkpoints.enabled": false,
  "chat.commandCenter.enabled": false,
  "chat.detectParticipant.enabled": false,
  "chat.disableAIFeatures": true,
  "chat.extensionTools.enabled": false,
  "comments.thread.confirmOnCollapse": "never",
  "containers.containers.label": "ContainerName",
  "containers.contexts.label": "Name",
  "cSpell.blockCheckingWhenTextChunkSizeGreaterThan": 1000,
  "cSpell.diagnosticLevel": "Hint",
  "cSpell.language": "en,de",
  "cSpell.unknownWords": "report-simple",
  "cSpell.userWords": [
    "dataviewjs",
    "horiceon",
    "saldo"
  ],
  "darkSynthwave84.disableGlow": true,
  "diffEditor.codeLens": true,
  "diffEditor.experimental.showMoves": true,
  "diffEditor.experimental.useTrueInlineView": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "dprint.experimentalLsp": true,
  "dprint.path": "~/.local/share/mise/installs/dprint/latest/dprint",
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit",
    "source.removeUnused.ts": "never",
    "source.removeUnusedImports": "never"
  },
  "editor.cursorBlinking": "solid",
  "editor.fontFamily": "DankMono Nerd Font",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.quickSuggestionsDelay": 0,
  "editor.renderWhitespace": "selection",
  "editor.rulers": [
    {
      "color": "#21242C",
      "column": 80
    },
    {
      "color": "#21242C",
      "column": 140
    }
  ],
  "editor.snippetSuggestions": "top",
  "editor.stickyScroll.enabled": false,
  "editor.suggest.preview": true,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "emmet.includeLanguages": {
    "svem": "html",
    "twig": "html"
  },
  "emojisense.languages": {
    "*": true
  },
  "errorLens.enabled": true,
  "errorLens.excludeBySource": [
    "cSpell"
  ],
  "errorLens.messageEnabled": true,
  "eslint.format.enable": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.patterns": {
    "*.db": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.s3db": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.sqlite": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.ts": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, bun.lock",
    "tsconfig.json": "tsconfig.*.json"
  },
  "explorer.sortOrder": "default",
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    ".chezmoiignore": "ignore",
    "*.html": "twig",
    "*.svg": "html"
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true,
    "**/.pnpm/**": true,
    "**/node_modules/**": true,
    "**/node_modules/*/**": true
  },
  "git.allowForcePush": true,
  "git.allowNoVerifyCommit": true,
  "git.autofetch": true,
  "git.blame.editorDecoration.enabled": false,
  "git.blame.statusBarItem.enabled": false,
  "git.confirmNoVerifyCommit": false,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "never",
  "git.replaceTagsWhenPull": true,
  "githubLocalActions.dockerDesktopPath": "/Applications/Docker.app",
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "gitlens.statusBar.enabled": false,
  "go.toolsManagement.autoUpdate": true,
  "headwind.runOnSave": true,
  "inline-parameters.fontStyle": "normal",
  "inline-parameters.hideSingleParameters": true,
  "inline-parameters.margin": "0 0.3em 0 0",
  "inline-parameters.padding": "0.2em",
  "inline-parameters.trailingCharacters": "",
  "intelephense.telemetry.enabled": false,
  "js/ts.implicitProjectConfig.checkJs": true,
  "js/ts.implicitProjectConfig.experimentalDecorators": true,
  "js/ts.implicitProjectConfig.target": "ESNext",
  "markdown.extension.theming.decoration.renderLink": true,
  "markdown.extension.toc.levels": "2..6",
  "mergeEditor.diffAlgorithm": "advanced",
  "notebook.formatOnSave.enabled": true,
  "php-cs-fixer.executablePath": "~/.config/composer/vendor/bin/php-cs-fixer",
  "playwright.reuseBrowser": true,
  "rainbow_csv.virtual_alignment_mode": "always",
  "redhat.telemetry.enabled": false,
  "scm.defaultViewMode": "tree",
  "security.workspace.trust.untrustedFiles": "open",
  "shellformat.useEditorConfig": true,
  "svelte.enable-ts-plugin": true,
  "svelte.plugin.svelte.runesLegacyModeCodeLens.enable": false,
  "task.allowAutomaticTasks": "off",
  "telemetry.feedback.enabled": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.enableImages": true,
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "terminal.integrated.fontSize": 11,
  "terminal.integrated.fontWeightBold": "400",
  "terminal.integrated.minimumContrastRatio": 7,
  "terminal.integrated.scrollback": 1000000,
  "terminal.integrated.tabs.enabled": false,
  "timetracker.dotTimeTrackerIndent": "  ",
  "todo-tree.filtering.excludeGlobs": [
    "**/node_modules/*/**",
    "**/.*/*/**"
  ],
  "todo-tree.general.statusBar": "tags",
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]",
    "MARK",
    "BOOKMARK"
  ],
  "typescript.experimental.expandableHover": true,
  "typescript.experimental.useTsgo": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.menuBarVisibility": "hidden",
  "window.newWindowProfile": "Default",
  "workbench.colorTheme": "Ayu Monokai",
  "workbench.commandPalette.experimental.enableNaturalLanguageSearch": false,
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.enablePreview": false,
  "workbench.editorAssociations": {
    "*.svg": "default"
  },
  "workbench.layoutControl.enabled": false,
  "workbench.startupEditor": "none",
  "workbench.tree.stickyScrollMaxItemCount": 3,
  "zenMode.fullScreen": false,
  "zenMode.showTabs": "single"
}
