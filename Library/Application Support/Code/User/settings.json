{
  "[astro]": {
    "editor.defaultFormatter": "astro-build.astro-vscode"
  },
  "[css][html][javascript][javascriptreact][typescript][typescriptreact][json][jsonc][markdown][scss][vue][yaml]": {
    "AkiraVoid.formatWithCLI.command": "prettier --ignore-unknown \"{file}\"",
    "editor.defaultFormatter": "AkiraVoid.format-with-cli"
  },
  "[csv][csv (semicolon)]": {
    "editor.inlayHints.maximumLength": 0
  },
  "[dockercompose][dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-containers"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[mjml]": {
    "editor.defaultFormatter": "mjmlio.vscode-mjml"
  },
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "mkhl.shfmt"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "[yaml]": {
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.formatOnSave": false
  },
  "chat.agent.enabled": false,
  "chat.checkpoints.enabled": false,
  "chat.commandCenter.enabled": false,
  "chat.detectParticipant.enabled": false,
  "chat.disableAIFeatures": true,
  "chat.extensionTools.enabled": false,
  "comments.thread.confirmOnCollapse": "never",
  "containers.containers.label": "ContainerName",
  "containers.contexts.label": "Name",
  "cSpell.blockCheckingWhenTextChunkSizeGreaterThan": 1000,
  "cSpell.language": "en,de",
  "cSpell.unknownWords": "report-simple",
  "cSpell.userWords": ["dataviewjs", "saldo"],
  "diffEditor.codeLens": true,
  "diffEditor.experimental.showMoves": true,
  "diffEditor.experimental.useTrueInlineView": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.cursorBlinking": "solid",
  "editor.fontFamily": "DankMono Nerd Font",
  "editor.fontLigatures": false,
  "editor.fontSize": 13,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.quickSuggestionsDelay": 0,
  "editor.rulers": [
    {
      "color": "#21242C",
      "column": 80
    },
    {
      "color": "#21242C",
      "column": 140
    }
  ],
  "editor.stickyScroll.enabled": false,
  "editor.suggest.preview": true,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "errorLens.excludeBySource": ["cSpell"],
  "errorLens.messageEnabled": true,
  "eslint.format.enable": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.patterns": {
    "*.db": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.s3db": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.sqlite": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.ts": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, bun.lock",
    "tsconfig.json": "tsconfig.*.json"
  },
  "explorer.sortOrder": "default",
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    ".chezmoiignore": "ignore",
    "*.svg": "html"
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true,
    "**/.pnpm/**": true,
    "**/node_modules/**": true,
    "**/node_modules/*/**": true
  },
  "git.allowForcePush": true,
  "git.allowNoVerifyCommit": true,
  "git.autofetch": true,
  "git.blame.statusBarItem.enabled": true,
  "git.confirmNoVerifyCommit": false,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "never",
  "git.replaceTagsWhenPull": true,
  "github-actions.workflows.pinned.refresh.enabled": true,
  "github.copilot.editor.enableAutoCompletions": false,
  "github.copilot.editor.enableCodeActions": false,
  "github.copilot.enable": {
    "*": false
  },
  "github.copilot.nextEditSuggestions.enabled": false,
  "github.copilot.renameSuggestions.triggerAutomatically": false,
  "githubIssues.queries": [
    {
      "groupBy": ["milestone"],
      "label": "My Issues",
      "query": "is:open assignee:${user} repo:${owner}/${repository}"
    },
    {
      "label": "Created Issues",
      "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
    },
    {
      "label": "Recent Issues",
      "query": "state:open repo:${owner}/${repository} sort:updated-desc"
    }
  ],
  "githubLocalActions.dockerDesktopPath": "/Applications/Docker.app",
  "githubPullRequests.createOnPublishBranch": "never",
  "githubPullRequests.fileListLayout": "flat",
  "githubPullRequests.pullBranch": "never",
  "go.toolsManagement.autoUpdate": true,
  "inline-parameters.fontStyle": "normal",
  "inline-parameters.hideSingleParameters": true,
  "inline-parameters.margin": "0 0.3em 0 0",
  "inline-parameters.padding": "0.2em",
  "inline-parameters.trailingCharacters": "",
  "intelephense.telemetry.enabled": false,
  "js/ts.implicitProjectConfig.checkJs": true,
  "js/ts.implicitProjectConfig.experimentalDecorators": true,
  "js/ts.implicitProjectConfig.target": "ESNext",
  "markdown.extension.theming.decoration.renderLink": true,
  "markdown.extension.toc.levels": "2..6",
  "mergeEditor.diffAlgorithm": "advanced",
  "notebook.formatOnSave.enabled": true,
  "package-json-upgrade.majorUpgradeColorOverwrite": "#FF4747",
  "package-json-upgrade.minorUpgradeColorOverwrite": "#FFC145",
  "package-json-upgrade.patchUpgradeColorOverwrite": "#40DC99",
  "package-json-upgrade.prereleaseUpgradeColorOverwrite": "#BC5EFF",
  "package-json-upgrade.skipNpmConfig": true,
  "php-cs-fixer.executablePath": "~/.config/composer/vendor/bin/php-cs-fixer",
  "playwright.reuseBrowser": true,
  "rainbow_csv.virtual_alignment_mode": "always",
  "redhat.telemetry.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "shellformat.useEditorConfig": true,
  "svelte.enable-ts-plugin": true,
  "task.allowAutomaticTasks": "off",
  "telemetry.feedback.enabled": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.enableImages": true,
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "terminal.integrated.fontSize": 11,
  "terminal.integrated.fontWeightBold": "400",
  "terminal.integrated.minimumContrastRatio": 7,
  "terminal.integrated.scrollback": 1000000,
  "timetracker.dotTimeTrackerIndent": "  ",
  "todo-tree.filtering.excludeGlobs": ["**/node_modules/*/**", "**/.*/*/**"],
  "todo-tree.general.statusBar": "tags",
  "typescript.experimental.expandableHover": true,
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "versionlens.deno.caching.duration": 5,
  "versionlens.docker.caching.duration": 10,
  "versionlens.golang.caching.duration": 5,
  "versionlens.npm.caching.duration": 5,
  "versionlens.pypi.caching.duration": 10,
  "versionlens.suggestions.showOnStartup": true,
  "versionlens.suggestions.showSuggestionsStats": true,
  "window.menuBarVisibility": "hidden",
  "workbench.colorTheme": "Evondev Dracula Darker Contrast New Color",
  "workbench.commandPalette.experimental.enableNaturalLanguageSearch": false,
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.enablePreview": false,
  "workbench.editorAssociations": {
    "*.svg": "default"
  },
  "workbench.layoutControl.enabled": false,
  "workbench.productIconTheme": "icons-carbon",
  "workbench.startupEditor": "none",
  "workbench.tree.stickyScrollMaxItemCount": 3,
  "zenMode.fullScreen": false,
  "zenMode.showTabs": "single"
}
