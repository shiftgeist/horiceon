{
  "[css][html][javascript][javascriptreact][typescript][typescriptreact][json][jsonc][markdown][scss][shellscript][vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[php]": {
    "editor.defaultFormatter": "junstyle.php-cs-fixer"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "cSpell.language": "en,de",
  "cSpell.userWords": [
    "captioner",
    "corepack",
    "esbuild",
    "Nextcloud",
    "openapi",
    "openapitools",
    "pinia",
    "typecheck",
    "vueuse"
  ],
  "debug.javascript.autoAttachFilter": "onlyWithFlag",
  "docker.containers.label": "ContainerName",
  "editor.accessibilitySupport": "off",
  "editor.cursorBlinking": "solid",
  "editor.fontFamily": "DankMono Nerd Font",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": true,
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "alt",
  "editor.quickSuggestionsDelay": 0,
  "editor.renderWhitespace": "all",
  "editor.rulers": [
    {
      "color": "#21242C",
      "column": 80
    }
  ],
  "editor.stickyScroll.enabled": true,
  "editor.suggest.preview": true,
  "editor.tabSize": 2,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.sortOrder": "default",
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    ".chezmoiignore": "ignore",
    "*.svg": "html"
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true,
    "**/.pnpm/**": true,
    "**/node_modules/**": true,
    "**/node_modules/*/**": true
  },
  "git.allowForcePush": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "githubPullRequests.createOnPublishBranch": "never",
  "githubPullRequests.pullBranch": "never",
  "gitlens.currentLine.enabled": false,
  "go.toolsManagement.autoUpdate": true,
  "Lua.telemetry.enable": false,
  "mergeEditor.diffAlgorithm": "experimental",
  "prettier.arrowParens": "avoid",
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "redhat.telemetry.enabled": false,
  "svelte.enable-ts-plugin": true,
  "svgPreview.autoOpen": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "terminal.integrated.fontWeightBold": "400",
  "terminal.integrated.minimumContrastRatio": 7,
  "terminal.integrated.scrollback": 1000000,
  "todomd.addCreationDate": true,
  "todomd.autoArchiveTasks": true,
  "todomd.completionDateIncludeTime": true,
  "todomd.counterBadgeEnabled": true,
  "todomd.creationDateIncludeTime": true,
  "todomd.defaultFile": "",
  "totalTypeScript.hiddenTips": [
    "passing-generics-to-types",
    "nonnullable-utility-type"
  ],
  "totalTypeScript.hideAllTips": false,
  "totalTypeScript.hideBasicTips": true,
  "volar.autoCompleteRefs": true,
  "volar.completion.preferredAttrNameCase": "auto-camel",
  "window.commandCenter": false,
  "window.menuBarVisibility": "hidden",
  "workbench.activityBar.location": "top",
  "workbench.colorTheme": "Dracula",
  "workbench.layoutControl.enabled": false,
  "workbench.startupEditor": "none"
}
