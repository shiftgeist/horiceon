{
  "[css][html][javascript][javascriptreact][typescript][typescriptreact][json][jsonc][markdown][scss][shellscript][vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-containers"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[mjml]": {
    "editor.defaultFormatter": "mjmlio.vscode-mjml"
  },
  "[php]": {
    "editor.defaultFormatter": "junstyle.php-cs-fixer"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "[yaml]": {
    "diffEditor.ignoreTrimWhitespace": false
  },
  "chat.commandCenter.enabled": false,
  "codemetrics.basics.CodeLensHiddenUnder": 5,
  "codemetrics.basics.EnabledForHTML": false,
  "comments.thread.confirmOnCollapse": "never",
  "containers.containers.label": "ContainerName",
  "cSpell.blockCheckingWhenTextChunkSizeGreaterThan": 1000,
  "cSpell.language": "en,de",
  "cSpell.reportUnknownWords": false,
  "debug.disassemblyView.showSourceCode": false,
  "debug.javascript.autoAttachFilter": "onlyWithFlag",
  "diffEditor.codeLens": true,
  "diffEditor.experimental.showMoves": true,
  "diffEditor.experimental.useTrueInlineView": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "docker.containers.label": "ContainerName",
  "editor.cursorBlinking": "solid",
  "editor.fontFamily": "DankMono Nerd Font",
  "editor.fontLigatures": false,
  "editor.fontSize": 13,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.quickSuggestionsDelay": 0,
  "editor.rulers": [
    {
      "color": "#21242C",
      "column": 80
    },
    {
      "color": "#21242C",
      "column": 140
    }
  ],
  "editor.stickyScroll.enabled": false,
  "editor.suggest.preview": true,
  "editor.tabSize": 2,
  "eslint.format.enable": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.sortOrder": "default",
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    ".chezmoiignore": "ignore",
    "*.svg": "html"
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true,
    "**/.pnpm/**": true,
    "**/node_modules/**": true,
    "**/node_modules/*/**": true
  },
  "git.allowForcePush": true,
  "git.allowNoVerifyCommit": true,
  "git.autofetch": true,
  "git.blame.statusBarItem.enabled": true,
  "git.confirmNoVerifyCommit": false,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "never",
  "git.replaceTagsWhenPull": true,
  "githubPullRequests.createOnPublishBranch": "never",
  "githubPullRequests.experimental.notificationsView": true,
  "githubPullRequests.fileListLayout": "tree",
  "githubPullRequests.pullBranch": "never",
  "go.toolsManagement.autoUpdate": true,
  "i18n-ally.displayLanguage": "en",
  "indentRainbow.colors": [
    "rgba(0, 0, 0, 0)",
    "rgba(38, 70, 83,0.15)",
    "rgba(42, 157, 143,0.15)",
    "rgba(233, 196, 106, 0.15)",
    "rgba(244, 162, 97,0.15)",
    "rgba(231, 111, 81,0.15)",
    "rgba(231, 111, 81,0.1)",
    "rgba(231, 111, 81,0.05)",
    "rgba(231, 111, 81,0.025)"
  ],
  "inline-parameters.hideSingleParameters": true,
  "markdown.extension.theming.decoration.renderLink": true,
  "markdown.extension.toc.levels": "2..6",
  "mergeEditor.diffAlgorithm": "advanced",
  "package-json-upgrade.majorUpgradeColorOverwrite": "#FF4747",
  "package-json-upgrade.minorUpgradeColorOverwrite": "#FFC145",
  "package-json-upgrade.patchUpgradeColorOverwrite": "#40DC99",
  "package-json-upgrade.prereleaseUpgradeColorOverwrite": "#BC5EFF",
  "package-json-upgrade.skipNpmConfig": true,
  "playwright.reuseBrowser": true,
  "prettier.arrowParens": "avoid",
  "prettier.experimentalTernaries": true,
  "prettier.printWidth": 100,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "redhat.telemetry.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "svelte.enable-ts-plugin": true,
  "symbols.hidesExplorerArrows": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.enableImages": true,
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "terminal.integrated.fontSize": 11,
  "terminal.integrated.fontWeightBold": "400",
  "terminal.integrated.minimumContrastRatio": 7,
  "terminal.integrated.scrollback": 1000000,
  "timetracker.dotTimeTrackerIndent": "  ",
  "typescript.experimental.expandableHover": true,
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.menuBarVisibility": "hidden",
  "workbench.activityBar.location": "top",
  "workbench.colorTheme": "Evondev Dracula Darker Contrast New Color",
  "workbench.commandPalette.experimental.enableNaturalLanguageSearch": false,
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.enablePreview": false,
  "workbench.iconTheme": "symbols",
  "workbench.layoutControl.enabled": false,
  "workbench.startupEditor": "none",
  "workbench.tree.stickyScrollMaxItemCount": 3,
  "zenMode.fullScreen": false,
  "zenMode.showTabs": "single"
}
