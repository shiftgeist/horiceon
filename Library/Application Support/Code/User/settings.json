{
  "[css][html][javascript][javascriptreact][typescript][typescriptreact][json][jsonc][markdown][scss][shellscript][vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[mjml]": {
    "editor.defaultFormatter": "mjmlio.vscode-mjml"
  },
  "[php]": {
    "editor.defaultFormatter": "junstyle.php-cs-fixer"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "code-runner.executorMap": {
    "javascript": "deno run --unstable -A --unstable-sloppy-imports",
    "typescript": "deno run --unstable -A --unstable-sloppy-imports"
  },
  "cSpell.language": "en,de",
  "cSpell.userWords": [
    "autofetch",
    "captioner",
    "chezmoiignore",
    "corepack",
    "esbenp",
    "esbuild",
    "fastify",
    "Fastify",
    "hono",
    "Hono",
    "iframes",
    "mjml",
    "nanostores",
    "Nextcloud",
    "openapi",
    "openapitools",
    "pinia",
    "preflights",
    "redhat",
    "scrollback",
    "tailwindcss",
    "timetracker",
    "todomd",
    "typecheck",
    "vueuse"
  ],
  "debug.javascript.autoAttachFilter": "onlyWithFlag",
  "diffEditor.codeLens": true,
  "docker.containers.label": "ContainerName",
  "editor.cursorBlinking": "solid",
  "editor.fontFamily": "DankMono Nerd Font",
  "editor.fontLigatures": false,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.quickSuggestionsDelay": 0,
  "editor.rulers": [
    {
      "color": "#21242C",
      "column": 80
    }
  ],
  "editor.stickyScroll.enabled": false,
  "editor.suggest.preview": true,
  "editor.tabSize": 2,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.sortOrder": "default",
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    ".chezmoiignore": "ignore",
    "*.svg": "html"
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true,
    "**/.pnpm/**": true,
    "**/node_modules/**": true,
    "**/node_modules/*/**": true
  },
  "foam.edit.linkReferenceDefinitions": "withExtensions",
  "git.allowForcePush": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "never",
  "githubPullRequests.createOnPublishBranch": "never",
  "githubPullRequests.pullBranch": "never",
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.defaultDateFormat": null,
  "gitlens.defaultDateStyle": "absolute",
  "gitlens.defaultTimeFormat": null,
  "gitlens.graph.dateStyle": "absolute",
  "gitlens.mode.statusBar.enabled": false,
  "gitlens.showWelcomeOnInstall": false,
  "go.toolsManagement.autoUpdate": true,
  "i18n-ally.displayLanguage": "de",
  "Lua.telemetry.enable": false,
  "markdown.extension.toc.levels": "2..6",
  "mergeEditor.diffAlgorithm": "advanced",
  "playwright.reuseBrowser": true,
  "prettier.arrowParens": "avoid",
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "redhat.telemetry.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "svelte.enable-ts-plugin": true,
  "svgPreview.autoOpen": false,
  "symbols.hidesExplorerArrows": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.enableImages": true,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "terminal.integrated.fontWeightBold": "400",
  "terminal.integrated.minimumContrastRatio": 7,
  "terminal.integrated.scrollback": 1000000,
  "timetracker.dotTimeTrackerIndent": "  ",
  "todomd.addCreationDate": true,
  "todomd.autoArchiveTasks": true,
  "todomd.completionDateIncludeTime": true,
  "todomd.counterBadgeEnabled": true,
  "todomd.creationDateIncludeTime": true,
  "todomd.defaultFile": "",
  "totalTypeScript.hiddenTips": [
    "passing-generics-to-types",
    "nonnullable-utility-type"
  ],
  "totalTypeScript.hideAllTips": false,
  "totalTypeScript.hideBasicTips": true,
  "volar.autoCompleteRefs": true,
  "volar.completion.preferredAttrNameCase": "auto-camel",
  "window.menuBarVisibility": "hidden",
  "workbench.activityBar.location": "top",
  "workbench.colorTheme": "Shades of Purple",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editorAssociations": {
    "git-rebase-todo": "gitlens.rebase"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.layoutControl.enabled": false,
  "workbench.startupEditor": "none",
  "workbench.tree.stickyScrollMaxItemCount": 2,
  "zenMode.fullScreen": false
}
