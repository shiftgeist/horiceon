source "$HOME/.profile"
source "${PROFILE:-$HOME/.config/zsh/.zshrc}"
source "$XDG_CONFIG_HOME/.aliases"
source "$XDG_CONFIG_HOME/.functions"
[ -f "$HOME/.extras" ] && source "$HOME/.extras"
[ -z "$DISPLAY" ] || xmodmap "$XDG_CONFIG_HOME/.Xmodmap"
[ -z "$DISPLAY" ] || xrdb "$XDG_CONFIG_HOME/Xresources"
