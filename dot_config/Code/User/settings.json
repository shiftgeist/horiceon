{
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "breadcrumbs.enabled": false,
  "debug.javascript.autoAttachFilter": "onlyWithFlag",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.cursorBlinking": "solid",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "'Dank Mono', 'Fira Code', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": true,
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "alt",
  "editor.quickSuggestionsDelay": 0,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    {
      "color": "#21242C",
      "column": 80
    }
  ],
  "editor.suggest.preview": true,
  "editor.tabSize": 2,
  "eslint.format.enable": true,
  "explorer.sortOrder": "default",
  "files.associations": {
    "*.svg": "html"
  },
  "git.allowForcePush": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "gitblame.infoMessageFormat": "${author.name}: \"${commit.summary}\" (${commit.hash_short})",
  "gitblame.statusBarMessageFormat": "${time.ago}",
  "gitlens.currentLine.enabled": false,
  "prettier.prettierPath": "~/.npm-global/bin/prettier",
  "svgPreview.autoOpen": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.minimumContrastRatio": 7,
  "terminal.integrated.scrollback": 1000000,
  "volar.completion.preferredTagNameCase": "pascal",
  "window.menuBarVisibility": "hidden",
  "workbench.colorTheme": "Shades of Purple"
}
