  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "breadcrumbs.enabled": false,
  "debug.javascript.autoAttachFilter": "onlyWithFlag",
  "editor.fontFamily": "'Dank Mono', 'Fira Code', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": true,
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "alt",
  "editor.quickSuggestionsDelay": 0,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    {
      "color": "#21242C",
      "column": 80
    }
  ],
  "editor.suggest.preview": true,
  "editor.suggestOnTriggerCharacters": false,
  "editor.tabSize": 2,
  "eslint.format.enable": true,
  "explorer.sortOrder": "default",
  "files.associations": {
    "*.svg": "html"
  },
  "git.allowForcePush": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "gitblame.infoMessageFormat": "${author.name}: \"${commit.summary}\" (${commit.hash_short})",
  "gitblame.statusBarMessageFormat": "${time.ago}",
  "gitlens.defaultDateShortFormat": "D.MM.YYYY",
  "gitlens.menus": {
    "editor": false,
    "editorGroup": {
      "blame": true,
      "compare": true
    },
    "editorTab": {
      "clipboard": false,
      "compare": false,
      "history": true,
      "remote": false
    },
    "explorer": {
      "clipboard": true,
      "compare": false,
      "history": true,
      "remote": false
    },
    "scm": {
      "authors": true
    },
    "scmGroup": {
      "compare": true,
      "openClose": true,
      "stash": true
    },
    "scmGroupInline": {
      "stash": true
    },
    "scmItem": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": false,
      "stash": true
    }
  },
  "gitlens.mode.active": "zen",
  "gitlens.mode.statusBar.enabled": false,
  "gitlens.statusBar.format": "${author}",
  "gutterpreview.showImagePreviewOnGutter": false,
  "prettier.prettierPath": "/home/fhungenberg/.volta/tools/image/packages/prettier/lib/node_modules/prettier",
  "svgPreview.autoOpen": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.minimumContrastRatio": 4.5,
  "terminal.integrated.mouseWheelScrollSensitivity": 3,
  "terminal.integrated.scrollback": 100000,
  "window.menuBarVisibility": "hidden",
  "workbench.colorCustomizations": {
    "parameterHints.hintBackground": "#00000000"
  },
  "workbench.colorTheme": "Andromeda Italic Bordered",
  "workbench.editor.untitled.hint": "hidden",
  "workbench.startupEditor": "newUntitledFile"
}
